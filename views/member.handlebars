<div id=playersearch" class="container table-responsive mt-3 mb-5" style="height:40vh;overflow-y:auto">
    <table class="table table-striped table-dark" id="playerTable">
    <thead>
        <tr>
            <th scope="col">Player Name</th>
            <th scope="col">PTS</th>
            <th scope="col">FG%</th>
            <th scope="col">3P%</th>
            <th scope="col">FT%</th>
            <th scope="col">REB</th>
            <th scope="col">AST</th>
            <th scope="col">STL</th>
            <th scope="col">BLK</th>
            <th scope="col">TOV</th>
            <th scope="col">Add To Five</th>
        </tr>
    </thead>
    {{#each players}}
    <tr id="{{PLAYER}}">
        <td>{{PLAYER}}</td>
        <td>{{PTS}}</td>
        <td>{{"FG%"}}</td>
        <td>{{"3P%"}}</td>
        <td>{{"FT%"}}</td>
        <td>{{REB}}</td>
        <td>{{AST}}</td>
        <td>{{STL}}</td>
        <td>{{BLK}}</td>
        <td>{{TOV}}</td>
        <td><i class="fa fa-heart fa-2x" aria-hidden="true" style="position: relative!important;top:15px" data-fav = false></i></td>
    </tr>
    {{/each}}
</table>
